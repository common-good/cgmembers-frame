  global $testOnly;
  if (!$a = r\acct($uid = t\uid($id))) return FALSE;

  if (!$testOnly) {
    $expires = REQUEST_TIME + R_SIGNIN_HOURS * HOUR_SECS;
    $oneTimePass = compact('pass', 'expires');
