/**
 * return URL (ARG)
 *
 * in: TEST ccpay ANonmemberDonatesToACcOkOrganizationByCreditCard
 *     TEST fbo ANonmemberDonatesToASponsoredOrganizationByCreditCard
 *     TEST guest ANonmemberPaysAMemberCompanyByCreditCard
 *     TEST guest ANonmemberPaysCommonGoodByCreditCard
 */
function returnURL($url) {
  global $testOnly, $testReturnUrl; if (!$testOnly) return FALSE;
  t\fixTestVars($url);
  if (!$res = t\eq(t('return URL'), BASE_URL . $url, $testReturnUrl)) {
    foreach (ray('want got', $url, $testReturnUrl) as $k => $urli) {
      if ($$k = strstr($urli, 'code=')) t\output("$k: " . pr(u\decryRay(substr($$k, 5)))); else t\output("No code= in $k: $urli");
    }
  }
  return $res;
}

/**
 * var (ARG) encrypts: (ARG)
 *
 * in: MAKE ccpay ANonmemberDonatesToACcOkOrganizationByCreditCard
 *     MAKE fbo ANonmemberDonatesToASponsoredOrganizationByCreditCard
 *     MAKE guest ANonmemberPaysAMemberCompanyByCreditCard
 *     MAKE guest ANonmemberPaysCommonGoodByCreditCard
 *     MAKE mfa AMemberSignsInWithAccountIDOnTheMemberSiteWithMFA
 *     MAKE mfa AMemberTypesTheWrongNonce
 *     MAKE nosignin AMemberDonatesFromAnEmailLink
 */
function varEncrypts($var, $ray) {
  global $testOnly, $testVars;
  $ray = $ray[0];
  t\fixData($ray);
  t\fixTestVars($ray);
  $want = u\cryRay($ray);
  return $testOnly ? t\eq(t('test Var'), nni($testVars, $var), $want) : ($testVars[$var] = $want or true);
}

/**
 * var (ARG) Pencrypts: (ARG)
 *
 * in: MAKE unsubscribe AMemberClicksAnUnsubscribeLink
 *     MAKE unsubscribe AMemberFollowsThroughOnUnsubscribing
 */
function varPencrypts($var, $ray) {
  global $testOnly, $testVars;
  if ($ok = varEncrypts($var, $ray) and !$testOnly) $testVars[$var] = CRYPT_FLAG . $testVars[$var];
  return $ok;
}

