function memberIsOnStepWithinSeconds($id, $step, $secs) {
  global $testOnly; if (!$testOnly) return FALSE;
  $us = new r\usd(r\acct(t\uid($id)));
  for ($i = 0; $i < $secs; $i++) {
    sleep(1);
    if ($us->step() == $step) return TRUE;
  }
  return FALSE;
}

